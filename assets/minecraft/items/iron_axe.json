{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:custom_model_data",
    "cases": [
      {
        "when": "hammer",
        "model": {
          "type": "minecraft:model",
          "model": "minecraft:item/iron_axe_hammer"
        }
      },
      {
        "when": "doubleheaded",
        "model": {
          "type": "minecraft:model",
          "model": "minecraft:item/iron_axe_doubleheaded"
        }
      },
      {
        "when": "spear2",
        "model": {
          "type": "minecraft:model",
          "model": "minecraft:item/iron_axe_big_spear2"
        }
      },
      {
        "when": "spear",
        "model": {
          "type": "minecraft:model",
          "model": "minecraft:item/iron_axe_big_spear"
        }
      },
      {
        "when": "poleaxe",
        "model": {
          "type": "minecraft:model",
          "model": "minecraft:item/iron_axe_big_poleaxe"
        }
      },
      {
        "when": "halberd",
        "model": {
          "type": "minecraft:model",
          "model": "minecraft:item/iron_axe_big_halberd"
        }
      }
    ],
    "fallback": {
      "type": "minecraft:model",
      "model": "minecraft:item/iron_axe"
    }
  }
}