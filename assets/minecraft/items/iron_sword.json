{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:custom_model_data",
    "cases": [
      {
        "when": "sickle",
        "model": {
          "type": "minecraft:model",
          "model": "minecraft:item/iron_sword_sickle"
        }
      },
      {
        "when": "rapier",
        "model": {
          "type": "minecraft:model",
          "model": "minecraft:item/iron_sword_rapier"
        }
      },
      {
        "when": "katana",
        "model": {
          "type": "minecraft:model",
          "model": "minecraft:item/iron_sword_katana"
        }
      },
      {
        "when": "falchion",
        "model": {
          "type": "minecraft:model",
          "model": "minecraft:item/iron_sword_falchion"
        }
      },
      {
        "when": "cutlass",
        "model": {
          "type": "minecraft:model",
          "model": "minecraft:item/iron_sword_cutlass"
        }
      },
      {
        "when": "scythe",
        "model": {
          "type": "minecraft:model",
          "model": "minecraft:item/iron_sword_big_scythe"
        }
      },
      {
        "when": "glaive",
        "model": {
          "type": "minecraft:model",
          "model": "minecraft:item/iron_sword_big_glaive"
        }
      },
      {
        "when": "longsword",
        "model": {
          "type": "minecraft:model",
          "model": "minecraft:item/iron_sword_longswo"
        }
      },
      {
        "when": "shortsword",
        "model": {
          "type": "minecraft:model",
          "model": "minecraft:item/iron_sword_shortsword"
        }
      }
    ],
    "fallback": {
      "type": "minecraft:model",
      "model": "minecraft:item/iron_sword"
    }
  }
}